        <SQLTask:SqlTaskData
          SQLTask:Connection="{EE9E78DF-F03B-4514-8606-EF96608D10A6}"
          SQLTask:SqlStatementSource="-- Parte 1: Leer archivo&#xA;USE Empresa&#xA;GO&#xA;IF EXISTS&#xA;(&#xA;&#x9;SELECT name&#xA;&#x9;FROM sys.tables&#xA;&#x9;WHERE name = 'T'&#xA;)&#xA;DROP TABLE [Empresa].[dbo].[T]&#xA;GO&#xA;SELECT *&#xA;INTO [Empresa].[dbo].[T]&#xA;FROM OPENROWSET&#xA;(&#xA;    'Microsoft.ACE.OLEDB.12.0',&#xA;    'Text;Database=C:\data\;FMT=CSVDelimited;HDR=YES',&#xA;    'SELECT * FROM [time_series_covid19_confirmed_global.csv]'&#xA;) AS SC&#xA;GO&#xA;&#xA;-- Parte 2: Guardar por UnPivot&#xA;IF EXISTS&#xA;(&#xA;&#x9;SELECT name&#xA;&#x9;FROM sys.tables&#xA;&#x9;WHERE name = 'CONFIRMADOS'&#xA;)&#xA;DROP TABLE CONFIRMADOS&#xA;GO&#xA;IF EXISTS&#xA;(&#xA;&#x9;SELECT t.name, c.name&#xA;&#x9;FROM sys.tables t&#xA;&#x9;INNER JOIN sys.columns c&#xA;&#x9;ON t.object_id = c.object_id&#xA;&#x9;WHERE&#xA;&#x9;&#x9;t.name = 'T'&#xA;&#x9;&#x9;AND&#xA;&#x9;&#x9;c.name = 'Province/State'&#xA;)&#xA;ALTER TABLE T&#xA;DROP COLUMN [Province/State]&#xA;GO&#xA;&#xA;-- Objetivo: hacer un UNpivot (Traer de las columnas hacia abajo)&#xA;DECLARE @strSQL AS VARCHAR(MAX)&#xA;DECLARE @strCampo AS CHAR(8)&#xA;&#xA;SET @strSQL = 'SELECT CONVERT(VARCHAR(32), [Country/Region]) AS [Country/Region], [Lat], [Long], CONVERT(DATE,[Fecha], 1) AS [Fecha], [Valor] INTO CONFIRMADOS FROM T '&#xA;SET @strSQL += 'UNPIVOT '&#xA;SET @strSQL += ' ( '&#xA;SET @strSQL += 'Valor FOR Fecha IN '&#xA;SET @strSQL += ' ( '&#xA;&#xA;DECLARE ucr_campos CURSOR FOR&#xA;SELECT c.name&#xA;FROM sys.tables t&#xA;INNER JOIN sys.columns c&#xA;ON t.object_id = c.object_id&#xA;WHERE&#xA;&#x9;t.name = 'T'&#xA;&#x9;AND&#xA;&#x9;c.name NOT IN ('Country/Region', 'Lat', 'Long')&#xA;&#xA;OPEN ucr_campos;&#xA;-- Extrae la primera fila del cursor y almacena su valor en la variable&#xA;FETCH NEXT FROM ucr_campos&#xA;INTO @strCampo;&#xA;-- El bucle se ejecuta mientras haya filas por leer. &#xA;-- @@FETCH_STATUS = 0 indica que el FETCH fue exitoso.&#xA;WHILE @@FETCH_STATUS=0&#xA;BEGIN&#xA;&#x9;SET @strSQL += CONCAT('[' , TRIM(@strCampo) , '], ')&#xA;&#xA;&#x9;FETCH NEXT FROM ucr_campos&#xA;&#x9;INTO @strCampo;&#xA;END&#xA;-- Para eliminar la última , del SELECT&#xA;SET @strSQL = SUBSTRING(@strSQL, 1, LEN(@strSQL) - 1);&#xA;CLOSE ucr_campos;&#xA;DEALLOCATE ucr_campos;&#xA;&#xA;&#xA;SET @strSQL += ' ) '&#xA;SET @strSQL += ') AS SCUnPivot'&#xA;&#xA;EXECUTE(@strSQL)" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
